{
  "compilerOptions": {
    "noEmit": true,
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "Bundler",
    "lib": ["esnext"],
    "strict": true,
    "types": [],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "moduleDetection": "force",
    "isolatedModules": true
  },
  "exclude": ["node_modules", "dist", ".wrangler", "**/*.test.ts", "**/*.spec.ts"]
}
